{
  "expo": {
    "name": "app-test-splash",
    "slug": "app-test-splash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bootsplash/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.app-test-splash"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bootsplash/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.apptestsplash"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/bootsplash/logo.png"
    },
    "plugins": [
      "expo-router",
      ["react-native-bootsplash", { "assetsDir": "assets/bootsplash" }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
